                <select asp-for="Programare.ServiciuID" class="form-control" asp-items="ViewBag.ServiciuID"></select>
            </div>
            <div class="form-group">
                <label asp-for="Programare.BisericaID" class="control-label"></label>
                <select asp-for="Programare.BisericaID" class="form-control" asp-items="ViewBag.BisericaID"></select>
            </div>
            <div class="form-group">
                <label asp-for="Programare.DataProgramare" class="control-label"></label>
