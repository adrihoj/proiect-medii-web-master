@page
@model Proiect.Pages.Programari.CreateModel


@{
    ViewData["Title"] = "Create";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Create</h1>

<h4>Programari</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Programare.EnoriasID" class="control-label"></label>
                <select asp-for="Programare.EnoriasID" class="form-control" asp-items="ViewBag.EnoriasID"></select>
            </div>
            <div class="form-group">
                <label asp-for="Programare.PreotID" class="control-label"></label>
                <select asp-for="Programare.PreotID" class="form-control" asp-items="ViewBag.PreotID"></select>
            </div>
            <div class="form-group">
